	mtkfb_pins_lcd_ldo18_out0: lcd_ldo18_out0_gpio {
			pins_cmd_dat {
					pinmux = <PINMUX_GPIO178__FUNC_GPIO178>;
					slew-rate = <1>;
					output-low;
