		};

	mtkfb_pins_lcd_ldo18_out1: lcd_ldo18_out1_gpio {
			pins_cmd_dat {
					pinmux = <PINMUX_GPIO178__FUNC_GPIO178>;
					slew-rate = <1>;
					output-high;
				};
		};

	mtkfb_pins_lcm_rst_out0: lcm_rst_out0_gpio {
			pins_cmd_dat {
